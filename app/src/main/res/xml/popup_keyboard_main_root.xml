<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:app="http://schemas.android.com/apk/res-auto"
    app:keyWidth="@dimen/width_keyboard_main_key"
    app:keyHeight="@dimen/height_keyboard_key">

    <Row>
        <Key
            app:keyOutputText="sqrt()"
            app:keyIcon="@drawable/ic_key_root2" />
        <Key
            app:keyOutputText="root(,3)"
            app:keyIcon="@drawable/ic_key_root3"/>
        <Key
            app:keyOutputText="root(,)"
            app:keyIcon="@drawable/ic_key_root" />
    </Row>

</Keyboard>